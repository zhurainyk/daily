/* 水平间距 */
.container {
  width: 100%;
  box-sizing: border-box;
  padding: 10rpx;
}
.container .tab {
  margin-bottom: 20rpx;
}
.container .tab .label {
  display: flex;
  justify-content: space-between;
  padding-bottom: 10rpx;
}
.container .tab .label text {
  color: #000;
}
.container .tab .label .icon {
  width: 50rpx;
  height: 50rpx;
  background: url(data:image/png;base64,) no-repeat center/cover;
}
.container .tab .value {
  width: 100%;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 10rpx;
  color: #666;
  border: 1px solid #ccc;
}
.container .tab .value.content {
  max-height: 444rpx;
  overflow: scroll;
}
.container .flex {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.container .primary {
  width: 180rpx;
  margin: 0;
}
.container .primary.right {
  width: 180rpx;
}
.container .history {
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .history .label {
  width: 90%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #666;
  font-size: 30rpx;
  padding: 10rpx 0;
}
.container .history .delete {
  color: #d00;
  font-size: 25rpx;
  padding: 0 10rpx;
  text-align: center;
  padding: 10rpx 0;
}

